---

apiVersion: v1
kind: Service
metadata:
  name: {{ nameserver }}
  namespace: {{ namespace }}
  labels:
    app: {{ nameserver }}
    service: {{ nameserver }}
spec:
  ports:
  - name: http-squash-api
    port: 8080
    protocol: TCP
    targetPort: 8080
  selector:
    app: {{ nameserver }}
    service: {{ nameserver }}
