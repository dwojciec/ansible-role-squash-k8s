---

apiVersion: route.openshift.io/v1
kind: Route
metadata:
annotations:
  openshift.io/host.generated: "true"
  name: {{ nameserver }}
  namespace: {{ namespace }}
  labels:
    app: {{ nameserver }}
    service: {{ nameserver }}
spec:
  port:
    targetPort: http-squash-api
  to:
    kind: Service
    name: {{ nameserver }}
    weight: 100
  wildcardPolicy: None 
